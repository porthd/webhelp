{
    "name": "porthd/webhelp",
    "type": "typo3-cms-extension",
    "description": "Build simple Webcomponents like icalendar with viewhelpers",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.d√ºddelei.de/",
    "version": "13.0.0",
    "keywords": [
        "TYPO3 CMS",
        "Frontendhelper",
        "webcomponents",
        "Extension"
    ],
    "authors": [
        {
            "name": "Dr. Dieter Porth",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^13.4 | ^12.4 | ^11.5 | ^10.4 | ^9.5"
	},
    "autoload": {
        "psr-4": {
            "Porthd\\Webhelp\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Porthd\\Webhelp\\Tests\\": "Tests/"
        }
    },
    "replace": {
        "typo3-ter/webhelp": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "webhelp"
        }
    }
}
