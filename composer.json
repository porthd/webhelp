{
    "name": "porthd/webhelp",
    "type": "typo3-cms-extension",
    "description": "Build simple Webcomponents like icalendar with viewhelpers",
    "authors": [
        {
            "name": "Dr. Dieter Porth",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^10.4"
    },
    "autoload": {
        "psr-4": {
            "Porthd\\Webhelp\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Porthd\\Webhelp\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/webhelp": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "webhelp"
        }
    }
}
